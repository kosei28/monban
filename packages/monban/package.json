{
    "name": "monban",
    "version": "0.1.3",
    "description": "Simple authentication library",
    "keywords": [
        "monban",
        "authentication",
        "auth",
        "login",
        "session"
    ],
    "author": "kosei28",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/kosei28/monban",
        "directory": "packages/monban"
    },
    "main": "dist/main.js",
    "types": "dist/main.d.ts",
    "exports": {
        ".": {
            "default": "./dist/main.js",
            "types": "./dist/main.d.ts"
        },
        "./types": {
            "default": "./dist/types.js",
            "types": "./dist/types.d.ts"
        },
        "./client": {
            "default": "./dist/client.js",
            "types": "./dist/client.d.ts"
        },
        "./providers/*": {
            "default": "./dist/providers/*.js",
            "types": "./dist/providers/*.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "types": [
                "./dist/types.d.ts"
            ],
            "client": [
                "./dist/client.d.ts"
            ],
            "providers/*": [
                "./dist/providers/*"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf dist && tsc",
        "test": "jest",
        "prepublishOnly": "npm run test && npm run build"
    },
    "devDependencies": {
        "@types/cookie": "^0.5.1",
        "@types/jest": "^29.5.1",
        "@types/uuid": "^9.0.1",
        "jest": "^29.5.0",
        "rimraf": "^5.0.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "cookie": "^0.5.0",
        "jose": "^4.14.4",
        "uuid": "^9.0.0"
    }
}
