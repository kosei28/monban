        "eslint-config-prettier": "^8.8.0",
        "husky": "^8.0.3",
        "prettier": "^2.8.8",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "@types/cookie": "^0.5.1",
        "cookie": "^0.5.0",
